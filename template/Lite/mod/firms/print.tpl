<!--buffer:thumb:0--><div class="{float}">{thumb}</div><!--buffer-->
<div id="print">
    <div class="printrow clearfix">
        {time} 
        <div class="title"><strong>{title}</strong></div>
        <div class="text">{image}{text}</div>
    </div>
    <div class="printline">
        <p>{copytext}</p>
        <p>{print_notice}</p>
    </div>
    <p class="link">{url}</p>
</div> 
